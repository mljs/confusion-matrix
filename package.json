{
  "name": "ml-confusion-matrix",
  "version": "0.4.1",
  "description": "Confusion matrix for supervised classification",
  "main": "lib/index.js",
  "module": "./lib-esm/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "src",
    "lib",
    "lib-esm"
  ],
  "scripts": {
    "build": "npm run tsc-esm && cheminfo-build --entry lib-esm/index.js --root ConfusionMatrix",
    "check-types": "tsc --noEmit",
    "clean": "rimraf lib lib-esm",
    "eslint": "eslint src",
    "eslint-fix": "npm run eslint -- --fix",
    "prepack": "npm run tsc",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "test": "npm run test-coverage && npm run eslint && npm run prettier && npm run check-types",
    "test-coverage": "jest --coverage",
    "test-only": "jest",
    "tsc": "npm run clean && npm run tsc-cjs && npm run tsc-esm",
    "tsc-cjs": "tsc --project tsconfig.cjs.json",
    "tsc-esm": "tsc --project tsconfig.esm.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mljs/confusion-matrix.git"
  },
  "keywords": [],
  "author": "Daniel Kostro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mljs/confusion-matrix/issues"
  },
  "homepage": "https://github.com/mljs/confusion-matrix#readme",
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.19.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.2.3",
    "cheminfo-build": "^1.1.11",
    "codecov": "^3.8.2",
    "eslint": "^8.28.0",
    "eslint-config-cheminfo-typescript": "^11.2.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.3"
  }
}
